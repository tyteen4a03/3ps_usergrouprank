<xen:navigation>
	<xen:breadcrumb href="{xen:adminlink '3ps-usergroup-ranks/delete', $userGroupRankId}">{$userGroupRankId}</xen:breadcrumb>
</xen:navigation>

<xen:require css="delete_confirmation.css" />

<xen:form action="{xen:adminlink '3ps-usergroup-ranks/delete', $userGroupRankId}" class="deleteConfirmForm formOverlay">

	<p>{xen:phrase 3ps_usergroup_ranks_delete_rank_confirm}:</p>
	<strong><a href="{xen:adminlink '3ps-usergroup-ranks/edit', $userGroupRankId}">{xen:raw $userGroupRankContent} ({xen:phrase 3ps_usergroup_ranks_rank_id}: {$userGroupRankId})</a></strong>

	<xen:submitunit save="{xen:phrase 3ps_usergroup_ranks_delete_usergroup_rank}" />
	<input type="hidden" name="rid" value="{$userGroupRankId}" />
	<input type="hidden" name="_xfConfirm" value="1" />
</xen:form>]]></template>
    <template title="3ps_usergroup_ranks_edit"><![CDATA[<xen:title>{xen:if '{$userGroupRank.rid}', '{xen:phrase 3ps_usergroup_ranks_edit_usergroup_rank}: {xen:phrase 3ps_usergroup_ranks_rank_id} {$userGroupRank.rid}', '{xen:phrase 3ps_usergroup_ranks_create_usergroup_rank}'}</xen:title>

<xen:form class="AutoValidator" action="{xen:adminlink '3ps-usergroup-ranks/save', $userGroupRank}" data-redirect="on">

	<ul class="tabs Tabs" data-panes="#3ps_usergroupRanksPane > li">
		<li class="active"><a title="{xen:phrase 3ps_usergroup_ranks_usergroup_rank} {xen:phrase settings}" class="Tooltip">{xen:phrase settings}</a></li>
		<li><a title="{xen:phrase 3ps_usergroup_ranks_display_usergroup_rank_if_user_or_usergroup}" class="Tooltip">{xen:phrase 3ps_usergroup_ranks_rank_display_condition}</a></li>
		<li><a class="Tooltip">{xen:phrase style_properties}</a></li>
	</ul>
	
	<ul id="3ps_usergroupRanksPane">
		<li>
			<xen:checkboxunit label="">
				<xen:option name="rank_active" selected="{$userGroupRank.rank_active}">{xen:phrase 3ps_usergroup_ranks_rank_is_active}</xen:option>
			</xen:checkboxunit>
			<xen:selectunit name="rank_type" label="{xen:phrase 3ps_usergroup_ranks_rank_type}" value="{$userGroupRank.rank_type}">
				<xen:option value="0" selected="{$userGroupRank.rank_type} == 0">{xen:phrase image}</xen:option>
				<xen:option value="1" selected="{$userGroupRank.rank_type} == 1">{xen:phrase text}</xen:option>
				<xen:explain>{xen:phrase 3ps_usergroup_ranks_rank_type_explain}</xen:explain>
			</xen:selectunit>
			<xen:textboxunit label="{xen:phrase 3ps_usergroup_ranks_rank_content}:" name="rank_content" value="{$userGroupRank.rank_content}">
				<xen:explain>{xen:phrase you_may_use_html}</xen:explain>
			</xen:textboxunit>
			<xen:if is="!{$userGroupRank.rid}">
				<input type="hidden" name="rid" value="0" />
			</xen:if>
		</li>
		<li>
			<xen:include template="3ps_usergroup_ranks_user_criteria" />
		</li>
		<li>
			<xen:textboxunit label="{xen:phrase 3ps_usergroup_ranks_rank_styling_class}:" name="rank_styling_class" value="{$userGroupRank.rank_styling_class}">
				<xen:explain>{xen:phrase 3ps_usergroup_ranks_rank_styling_class_explain}</xen:explain>
			</xen:textboxunit>
		</li>
	</ul>

	<xen:submitunit save="{xen:phrase 3ps_usergroup_ranks_save_usergroup_rank}"></xen:submitunit>
	<xen:controlunit label="{xen:phrase note}:">{xen:phrase 3ps_usergroup_ranks_no_criteria_warning}</xen:controlunit>
</xen:form>]]></template>
