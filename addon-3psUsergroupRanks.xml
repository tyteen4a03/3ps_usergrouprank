    <template title="3ps_usergroup_ranks_displaybit" version_id="1" version_string="1.1.0"><![CDATA[<xen:require css="3ps_usergroup_ranks.css" />
<ul class="userGroupRanks posPad{$ugrList_posPad}">
	<xen:foreach loop="$userGroupRanks" value="$userGroupRank">
		<li {xen:if '{$userGroupRank.rank_styling_class}', 'class="{$userGroupRank.rank_styling_class}"', ''}>
			<xen:if is="{$userGroupRank.rank_type} === 0">
				<img src="{$userGroupRank.rank_content}" />
			<xen:else />
				{xen:raw $userGroupRank.rank_content}
			</xen:if>
		</li>
			{xen:raw $userGroupRank.content_render}
	</xen:foreach>
</ul>]]></template>
